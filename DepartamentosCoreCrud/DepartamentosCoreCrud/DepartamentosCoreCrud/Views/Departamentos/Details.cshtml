@model Departamento

<h1>Detalles</h1>

<a asp-controller="Departamentos" asp-action="Index">Volver al listado</a>

<hr />

<ul class=" list-group">
    <li class=" list-group-item">ID Departamento: @Model.Numero</li>
    <li class=" list-group-item-primary">Nombre: @Model.Nombre</li>
    <li class=" list-group-item-warning">Localidad: @Model.Localidad</li>
</ul>

