@model List<Hospital>
<h1>San Carlos</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <td>Nombre</td>
            <td>Direccion</td>
            <td>Telefono</td>
            <td>Numero de camas</td>
        </tr>
    </thead>
    <tbody>
        @foreach (Hospital hosp in Model)
        {
            <tr>
                <th>@hosp.Nombre</th>
                <th>@hosp.Direccion</th>
                <th>@hosp.Telefono</th>
                <th>@hosp.Num_cama</th>
            </tr>
        }
    </tbody>
</table>
