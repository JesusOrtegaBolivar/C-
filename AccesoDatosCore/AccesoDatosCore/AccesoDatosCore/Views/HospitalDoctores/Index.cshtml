@model List<Doctor>
<h1>Doctores</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <td>Apellido</td>
            <td>Funcion</td>
            <td>Codigo de hospital</td>
        </tr>
    </thead>
    <tbody>
        @foreach (Doctor doc in Model)
        {
            <tr>
                <th>@doc.Apellido</th>
                <th>@doc.Especialidad</th>
                <th>@doc.Hospital_Cod</th>
            </tr>
        }
    </tbody>
</table>

<hr />
<a class="btn btn-danger" asp-area="" asp-controller="Hospital" asp-action="Menu">Menu Hospitales</a>

