@model List<Plantilla>
<h1>Plantilla</h1>


<table class="table table-dark">
    <thead>
        <tr>
            <td>Apellido</td>
            <td>Funcion</td>
            <td>Codigo de hospital</td>
        </tr>
    </thead>
    <tbody>
        @foreach (Plantilla plant in Model)
        {
            <tr>
                <th>@plant.Apellido</th>
                <th>@plant.Funcion</th>
                <th>@plant.Hospital_cod</th>
            </tr>
        }
    </tbody>
</table>

<hr />
<a class="btn btn-danger" asp-area="" asp-controller="Hospital" asp-action="Menu">Menu Hospitales</a>